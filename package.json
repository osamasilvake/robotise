{
	"name": "roc",
	"version": "2.3.0",
	"private": true,
	"scripts": {
		"start": "react-scripts start",
		"build": "react-scripts build",
		"test": "react-scripts test",
		"test:ci": "CI=true react-scripts test",
		"pretty:check": "prettier --check './**/*.{js,jsx,ts,tsx,json}'",
		"pretty:apply": "prettier --write './**/*.{js,jsx,ts,tsx,json}'",
		"lint:es": "npx eslint './**/*.{js,jsx,ts,tsx}' --quiet",
		"lint:es:fix": "npx eslint './**/*.{js,jsx,ts,tsx}' --fix",
		"scan:all": "yarn pretty:apply && yarn lint:es && yarn test:ci",
		"docker:run": "cd docker && docker compose --file Docker.run.yml up",
		"docker:build": "cd docker && docker compose --file Docker.yml up --build",
		"release": "standard-version",
		"release:major": "yarn release -- --release-as major",
		"release:minor": "yarn release -- --release-as minor",
		"release:patch": "yarn release -- --release-as patch"
	},
	"dependencies": {
		"@date-io/date-fns": "^2.11.0",
		"@emotion/react": "^11.7.1",
		"@emotion/styled": "^11.6.0",
		"@mui/icons-material": "^5.2.5",
		"@mui/material": "^5.2.5",
		"@mui/styles": "^5.2.3",
		"@reduxjs/toolkit": "^1.7.1",
		"@types/dompurify": "^2.3.2",
		"@types/jest": "^27.0.3",
		"@types/jsonapi-serializer": "^3.6.5",
		"@types/node": "^17.0.2",
		"@types/react": "^17.0.37",
		"@types/react-csv": "^1.1.2",
		"@types/react-dom": "^17.0.11",
		"@types/react-redux": "^7.1.20",
		"@types/react-router-dom": "^5.3.2",
		"@types/redux-logger": "^3.0.9",
		"@types/redux-mock-store": "^1.0.3",
		"axios": "^0.24.0",
		"clsx": "^1.1.1",
		"date-fns": "^2.27.0",
		"dompurify": "^2.3.4",
		"i18next": "^21.6.3",
		"jsonapi-serializer": "^3.6.7",
		"loglevel": "^1.8.0",
		"moment": "^2.29.1",
		"react": "^17.0.2",
		"react-csv": "^2.2.1",
		"react-dom": "^17.0.2",
		"react-helmet-async": "^1.2.2",
		"react-i18next": "^11.15.1",
		"react-redux": "^7.2.6",
		"react-router-dom": "^6.2.1",
		"react-scripts": "5.0.0",
		"redux": "^4.1.2",
		"redux-devtools-extension": "^2.13.9",
		"redux-logger": "^3.0.6",
		"redux-mock-store": "^1.5.4",
		"redux-thunk": "^2.4.1",
		"typescript": "~4.5.4"
	},
	"devDependencies": {
		"@commitlint/cli": "^15.0.0",
		"@commitlint/config-conventional": "^15.0.0",
		"@typescript-eslint/eslint-plugin": "^5.8.0",
		"@typescript-eslint/parser": "^5.8.0",
		"eslint-config-prettier": "^8.3.0",
		"eslint-plugin-jsx-a11y": "^6.5.1",
		"eslint-plugin-prettier": "^4.0.0",
		"eslint-plugin-react": "^7.27.1",
		"eslint-plugin-react-hooks": "^4.3.0",
		"eslint-plugin-simple-import-sort": "^7.0.0",
		"husky": "^7.0.4",
		"lint-staged": ">=12",
		"prettier": "^2.5.1",
		"standard-version": "^9.3.2"
	},
	"browserslist": {
		"production": [
			">0.2%",
			"not dead",
			"not op_mini all"
		],
		"development": [
			"last 1 chrome version",
			"last 1 firefox version",
			"last 1 safari version"
		]
	}
