		"lint-staged": ">=13",
		"prettier": "^2.7.1",
		"react-app-rewired": "^2.2.1",
		"standard-version": "^9.5.0"
	},
	"browserslist": {
		"production": [
			">0.2%",
			"not dead",
			"not op_mini all"
		],
		"development": [
			"last 1 chrome version",
			"last 1 firefox version",
			"last 1 safari version"
		]
	}
