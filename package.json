{
	"name": "roc",
	"version": "2.10.0",
	"private": true,
	"scripts": {
		"start": "react-app-rewired start",
		"build": "react-app-rewired build",
		"test": "react-app-rewired test --transformIgnorePatterns \"node_modules/(?!axios)/\"",
		"test:ci": "CI=true react-app-rewired test --transformIgnorePatterns \"node_modules/(?!axios)/\"",
		"pretty:check": "prettier --check './**/*.{js,jsx,ts,tsx,json}'",
		"pretty:apply": "prettier --write './**/*.{js,jsx,ts,tsx,json}'",
		"lint:es": "npx eslint './**/*.{js,jsx,ts,tsx}' --quiet",
		"lint:es:fix": "npx eslint './**/*.{js,jsx,ts,tsx}' --fix",
		"scan:all": "yarn pretty:apply && yarn lint:es && yarn test:ci",
		"docker:run": "cd docker && docker compose --file Docker.run.yml up",
		"docker:build": "cd docker && docker compose --file Docker.yml up --build",
		"release": "standard-version",
		"release:major": "yarn release -- --release-as major",
		"release:minor": "yarn release -- --release-as minor",
		"release:patch": "yarn release -- --release-as patch",
		"analyze": "source-map-explorer 'build/static/js/*.js'"
	},
	"dependencies": {
		"@emotion/react": "^11.10.6",
		"@emotion/styled": "^11.10.6",
		"@mui/icons-material": "^5.11.11",
		"@mui/material": "^5.11.12",
		"@mui/styles": "^5.11.12",
		"@mui/x-date-pickers": "^6.0.0",
		"@reduxjs/toolkit": "^1.9.3",
		"@types/dom-to-image": "^2.6.4",
		"@types/dompurify": "^2.4.0",
		"@types/jest": "^29.4.0",
		"@types/jsonapi-serializer": "^3.6.5",
		"@types/node": "^18.14.6",
		"@types/react": "^18.0.28",
		"@types/react-csv": "^1.1.3",
		"@types/react-dom": "^18.0.11",
		"@types/react-redux": "^7.1.25",
		"@types/react-router-dom": "^5.3.3",
		"@types/redux-logger": "^3.0.9",
		"@types/redux-mock-store": "^1.0.3",
		"axios": "^1.3.4",
		"clsx": "^1.2.1",
		"dayjs": "^1.11.7",
		"dom-to-image": "^2.6.0",
		"dompurify": "^3.0.1",
		"i18next": "^22.4.11",
		"jsonapi-serializer": "^3.6.7",
		"loglevel": "^1.8.1",
		"qrcode": "^1.5.1",
		"react": "^18.2.0",
		"react-csv": "^2.2.2",
		"react-dom": "^18.2.0",
		"react-helmet-async": "^1.3.0",
		"react-i18next": "^12.2.0",
		"react-redux": "^8.0.5",
		"react-router-dom": "^6.8.2",
		"react-scripts": "5.0.1",
		"recharts": "^2.4.3",
		"redux": "^4.2.1",
		"redux-devtools-extension": "^2.13.9",
		"redux-logger": "^3.0.6",
		"redux-mock-store": "^1.5.4",
		"redux-thunk": "^2.4.2",
		"source-map-explorer": "^2.5.3",
		"typescript": "~4.9.5",
		"yaml": "^2.2.1"
	},
	"devDependencies": {
		"@commitlint/cli": "^17.4.4",
		"@commitlint/config-conventional": "^17.4.4",
		"@types/qrcode": "^1.5.0",
		"@typescript-eslint/eslint-plugin": "^5.54.1",
		"@typescript-eslint/parser": "^5.54.1",
		"babel-plugin-import": "^1.13.6",
		"customize-cra": "^1.0.0",
		"eslint-config-prettier": "^8.7.0",
		"eslint-plugin-jsx-a11y": "^6.7.1",
		"eslint-plugin-prettier": "^4.2.1",
		"eslint-plugin-react": "^7.32.2",
		"eslint-plugin-react-hooks": "^4.6.0",
		"eslint-plugin-simple-import-sort": "^10.0.0",
		"husky": "^8.0.3",
		"lint-staged": ">=13",
		"prettier": "^2.8.4",
		"react-app-rewired": "^2.2.1",
		"standard-version": "^9.5.0"
	},
	"browserslist": {
		"production": [
			">0.2%",
			"not dead",
			"not op_mini all"
		],
		"development": [
			"last 1 chrome version",
			"last 1 firefox version",
			"last 1 safari version"
		]
	}
}
