{
	"name": "roc",
	"version": "2.2.0",
	"private": true,
	"scripts": {
		"start": "react-scripts start",
		"build": "react-scripts build",
		"test": "react-scripts test",
		"test:ci": "CI=true react-scripts test",
		"pretty:check": "prettier --check './**/*.{js,jsx,ts,tsx,json}'",
		"pretty:apply": "prettier --write './**/*.{js,jsx,ts,tsx,json}'",
		"lint:es": "npx eslint './**/*.{js,jsx,ts,tsx}' --quiet",
		"lint:es:fix": "npx eslint './**/*.{js,jsx,ts,tsx}' --fix",
		"scan:all": "yarn pretty:apply && yarn lint:es && yarn test:ci",
		"docker:run": "cd docker && docker compose --file Docker.run.yml up",
		"docker:build": "cd docker && docker compose --file Docker.yml up --build",
		"release": "standard-version",
		"release:major": "yarn release -- --release-as major",
		"release:minor": "yarn release -- --release-as minor",
		"release:patch": "yarn release -- --release-as patch"
	},
	"dependencies": {
		"@date-io/date-fns": "^2.11.0",
		"@emotion/react": "^11.4.1",
		"@emotion/styled": "^11.3.0",
		"@material-ui/core": "^5.0.0-beta.2",
		"@material-ui/icons": "^5.0.0-beta.0",
		"@material-ui/styles": "^5.0.0-beta.0",
		"@reduxjs/toolkit": "^1.6.1",
		"@types/dompurify": "^2.2.3",
		"@types/jest": "^27.0.1",
		"@types/jsonapi-serializer": "^3.6.5",
		"@types/node": "^16.7.6",
		"@types/react": "^17.0.19",
		"@types/react-csv": "^1.1.2",
		"@types/react-dom": "^17.0.9",
		"@types/react-redux": "^7.1.18",
		"@types/react-router-dom": "^5.1.8",
		"@types/redux-logger": "^3.0.9",
		"@types/redux-mock-store": "^1.0.3",
		"axios": "^0.21.1",
		"clsx": "^1.1.1",
		"date-fns": "^2.23.0",
		"dompurify": "^2.3.1",
		"i18next": "^20.4.0",
		"jsonapi-serializer": "^3.6.7",
		"jwt-decode": "^3.1.2",
		"loglevel": "^1.7.1",
		"moment": "^2.29.1",
		"react": "^17.0.2",
		"react-csv": "^2.0.3",
		"react-dom": "^17.0.2",
		"react-helmet-async": "^1.1.2",
		"react-i18next": "^11.11.4",
		"react-redux": "^7.2.4",
		"react-router-dom": "^5.2.1",
		"react-scripts": "4.0.3",
		"redux": "^4.1.1",
		"redux-devtools-extension": "^2.13.9",
		"redux-logger": "^3.0.6",
		"redux-mock-store": "^1.5.4",
		"redux-thunk": "^2.3.0",
		"typescript": "~4.4.2"
	},
	"devDependencies": {
		"@commitlint/cli": "^13.1.0",
		"@commitlint/config-conventional": "^13.1.0",
		"@typescript-eslint/eslint-plugin": "^4.29.3",
		"@typescript-eslint/parser": "^4.29.3",
		"eslint": "^7.32.0",
		"eslint-config-prettier": "^8.3.0",
		"eslint-plugin-jsx-a11y": "^6.4.1",
		"eslint-plugin-prettier": "^3.4.1",
		"eslint-plugin-react": "^7.25.1",
